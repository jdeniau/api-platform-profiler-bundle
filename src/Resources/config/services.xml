<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:twig="http://symfony.com/schema/dic/twig"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Mapado\ApiPlatformProfilerBundle\DataCollector\RequestCollector" public="false">
            <argument type="service" id="api_platform.metadata.resource.name_collection_factory" />
            <argument type="service" id="api_platform.metadata.resource.metadata_factory" />

            <!-- priority="300" -->
            <tag name="data_collector"
                template="@MapadoApiPlatformProfilerBundle/Resources/views/data_collector/request.html.twig"
                id="mapado_api_platform_profiler.request_collector"
                />
        </service>
    </services>
</container>
